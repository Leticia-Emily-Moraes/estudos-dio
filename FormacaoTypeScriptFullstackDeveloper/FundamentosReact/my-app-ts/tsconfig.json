{
	"compilerOptions": {
	  "target": "es5",
	  "module": "esnext",
	  "strict": true,
	  "jsx": "react-jsx",
	  "moduleResolution": "node",
	  "types": ["jest", "node"],
	  "typeRoots": ["./node_modules/@types", "./node_modules/@types/node"],
	  "baseUrl": "./",
	  "paths": {
		"@/*": ["src/*"]
	  },
	  "allowSyntheticDefaultImports": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"include": ["src", "src/types"]
  }
  